    "        threeMultiple +=n\n",
    "    elif n %5==0: \n",
    "        fourMultiple+=n\n",
    "    else:\n",
    "        sumOfOthers +=n\n",
    "print(threeMultiple)\n",
    "print(fourMultiple)\n",
    "print(sumOfOthers)"
